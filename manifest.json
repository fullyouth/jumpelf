{
  "manifest_version": 2,
  "name": "跳转精灵",
  "version": "2.0",
  "description": "An extension for jumping to URLs based on commands.",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "跳转精灵",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "webNavigation"
  ]
}